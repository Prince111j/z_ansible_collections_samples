//SYSTSIN  DD  *
DSN SYSTEM ({{item.db2_system.subsys}})
BIND  PLAN({{item.db2_system.plan}}) +
   PKLIST({{item.db2_system.plan_pklist}}) +
   QUALIFIER({{item.db2_system.qualifier}}) +
   OWNER({{item.db2_system.sqlid}}) +
   ACTION({{item.db2_system.plan_action}}) +
   ISOLATION(CS) +
   RELEASE(COMMIT) +
   ENCODING(EBCDIC)
END
